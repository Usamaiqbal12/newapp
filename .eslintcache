s from \"./PrivateRoutes\";\r\nimport DiscussionMode from \"./components/dataset/DiscussionMode\";\r\nimport Footer from \"./components/Footer\";\r\n\r\nfunction App() {\r\n  const [, dispatch] = useStateValue();\r\n  useEffect(() => {\r\n    let mounted = true;\r\n    if (localStorage.getItem(\"jwt\")) {\r\n      getUser().then((data) => {\r\n        if (mounted) {\r\n          dispatch({\r\n            type: \"ADDUSER\",\r\n            data: data,\r\n          });\r\n        }\r\n      });\r\n      datasetListFunc().then((items) => {\r\n        if (mounted) {\r\n          dispatch({\r\n            type: \"ADDDATASET\",\r\n            data: items.data,\r\n          });\r\n        }\r\n      });\r\n    }\r\n    return () => (mounted = false);\r\n  }, []);\r\n  return (\r\n    <>\r\n      <NavBar />\r\n      <main className=\"container\">\r\n        <Switch>\r\n          <Route path=\"/signin\" component={SignIn} />\r\n          <Route path=\"/signup\" component={SignUp} />\r\n          <Route path=\"/discussionmode/:id\" component={DiscussionMode} />\r\n          <PrivateRoutes path=\"/profile\" component={Profile} />\r\n          <PrivateRoutes path=\"/createsearch\" component={CreateDataset} />\r\n          <PrivateRoutes path=\"/manual\" component={Manual} />\r\n          <PrivateRoutes exact path=\"/dataset\" component={ListDataset} />\r\n          <PrivateRoutes\r\n            path=\"/dataset/detail/:id\"\r\n            component={DatasetDetails}\r\n          />\r\n          <PrivateRoutes path=\"/new\" component={Params} />\r\n          <PrivateRoutes path=\"/editprofile\" component={EditProfile} />\r\n        </Switch>\r\n      </main>\r\n      <Footer />\r\n    </>\r\n  );\r\n}\r\nexport default App;\r\n","C:\\Users\\Usama\\Desktop\\NewApp\\newapp\\src\\components\\SignUp.js",[],"C:\\Users\\Usama\\Desktop\\NewApp\\newapp\\src\\components\\userProfile.js",[],"C:\\Users\\Usama\\Desktop\\NewApp\\newapp\\src\\components\\Header.js",[],"C:\\Users\\Usama\\Desktop\